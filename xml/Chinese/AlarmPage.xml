			<AlarmOutCopyWeekOpt7>每天</AlarmOutCopyWeekOpt7>
			<btnAlarmOutCopy>复制</btnAlarmOutCopy>
			<Segment>时间段</Segment>
			<StartTime>开始时间</StartTime>
			<EndTime>结束时间</EndTime>
			<SegmentOne>时间段一</SegmentOne>
			<HourOne>时</HourOne>
			<MinOne>分</MinOne>
